{
  "name": "@sandshrew/server",
  "version": "2.0.0",
  "description": "npm packages grab tool",
  "author": "tchen",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch \"src/**/*.ts\" --exec node \"--import=./register-loader.js\" \"./src/dev.ts\"",
    "dev:docker": "nodemon --watch \"src/**/*.ts\" --exec node \"--import=./register-loader.js\" \"./src/dev_docker.ts\"",
    "build": "rollup -c rollup.config.js",
    "start": "node start.js"
  },
  "files": [
    "dist",
    "src",
    "start.js"
  ],
  "inject-workspace-packages": true,
  "main": "index.js",
  "dependencies": {
    "@fastify/compress": "^7.0.3",
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.3.1",
    "@fastify/static": "^7.0.4",
    "@shockpkg/archive-files": "^3.2.5",
    "axios": "1.7.9",
    "extract-zip": "^2.0.1",
    "fastify": "^4.29.0",
    "fee-local-npm": "^3.0.7",
    "pino-pretty": "^13.0.0",
    "rimraf": "^5.0.10",
    "semver": "^7.7.1",
    "yaml": "^2.7.0",
    "zx": "^8.3.2"
  },
  "devDependencies": {
    "@npm/types": "^2.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@sandshrew/types": "workspace:*",
    "@types/semver": "^7.5.8",
    "nodemon": "^3.1.9",
    "rollup": "^4.34.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.14.0"
  },
  "homepage": "https://github.com/abc3660170/npm-front#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abc3660170/npm-front.git"
  }
}
